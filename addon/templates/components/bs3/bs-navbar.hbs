<div class={{if this.fluid "container-fluid" "container"}}>
  {{yield
    (hash
      toggle=(component this.toggleComponent onClick=(action "toggleNavbar") collapsed=_collapsed)
      content=(component this.contentComponent collapsed=_collapsed onHidden=onCollapsed onShown=onExpanded)
      nav=(component
        this.navComponent
        linkToComponent=(component this.linkToComponent onCollapse=(action "collapse"))
      )
      collapse=(action "collapse")
      expand=(action "expand")
      toggleNavbar=(action "toggleNavbar")
    )
  }}
</div>
